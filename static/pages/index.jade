extends ../layout  
block content  
  div(class='container')
    h1 #{contentTitle} 
    p #{description} 
    if isLogin == true
      a(href='#' class='login-txt') #{userName}
      a(href='/logout' class='regist-txt') logout
    else
      a(href='/login' class='login-txt') login
      a(href='/regist' class='regist-txt') regist
  br
  each img in imgs  
      img(src="images/#{img}" class="row-img")